{
    "manifest_version": 3,
    "name": "Review Macro Extension",
    "version": "1.0",
    "description": "A productivity extension for reviewing and managing submissions quickly.",
    "permissions": ["activeTab", "storage"],
    "action": {
    "default_popup": "popup.html"
    },
    "content_scripts": [
    {
        "matches": ["*://*/*"],
        "js": ["content.js"]
    }
    ]
}
